{
    "name": "creativitykills/filament-artisan",
    "type": "library",
    "description": "Simple but yet powerful library for running some artisan commands for FilamentPHP",
    "keywords": [
        "php",
        "laravel",
        "artisan",
        "commands",
        "gui",
        "filament"
    ],
    "license": "MIT",
    "homepage": "https://github.com/CreativityKills/filament-artisan",
    "support": {
        "issues": "https://github.com/CreativityKills/filament-artisan/issues",
        "source": "https://github.com/CreativityKills/filament-artisan"
    },
    "autoload": {
        "psr-4": {
            "CreativityKills\\FilamentArtisan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CreativityKills\\FilamentArtisan\\FilamentArtisanServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "filament/filament": "^4.0",
        "calebporzio/sushi": "*",
        "spatie/laravel-package-tools": "^1.19",
        "tomatophp/filament-developer-gate": "^4.0",
        "tomatophp/console-helpers": "^1.1"
    }
}
